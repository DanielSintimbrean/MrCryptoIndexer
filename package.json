{
  "name": "mrcryptoindexer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn -r tsconfig-paths/register --transpile-only src/server",
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc && npm run build:schema",
    "build:schema": "ts-node src/schema/index.ts",
    "generate": "npm -s run generate:prisma",
    "generate:prisma": "prisma generate",
    "foo": "ts-node -r tsconfig-paths/register ./src/indexer/index.ts",
    "bar": "ts-node -r tsconfig-paths/register ./src/indexer/script.ts",
    "prisma:reset": "prisma db push --force-reset"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "20.1.0",
    "cross-env": "7.0.3",
    "prisma": "^4.13.0",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@graphql-tools/schema": "^9.0.19",
    "@pothos/core": "^3.30.0",
    "@pothos/plugin-prisma": "^3.50.0",
    "@prisma/client": "^4.13.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.21.3",
    "graphql-yoga": "3.9.1",
    "viem": "^0.3.17"
  }
}
